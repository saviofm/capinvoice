{
  "name": "capinvoice",
  "version": "1.0.0",
  "description": "Cap Invoice",
  "repository": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "6.1.3",
    "@sap/cds-dk": "^6.1.5",
    "@sap/cds-odata-v2-adapter-proxy": "1.9.10",
    "@sap/textbundle": "4.1.0",
    "@sap/xsenv": "3.3.2",
    "@sap/xssec": "3.2.13",
    "@types/node-fetch": "^2.6.1",
    "cds-swagger-ui-express": "^0.4.0",
    "cors": "^2.8.5",
    "express": "^4",
    "hdb": "0.19.3",
    "passport": "^0.5.2"
  },
  "devDependencies": {
    "@sap/cds-compiler": "3.3.2",
    "@sap/eslint-plugin-cds": "2.3.3",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/hdi-deploy": "4.4.1",
    "@sap/ux-specification": "^1.100.3",
    "@sap/ux-ui5-tooling": "^1.5.5",
    "@sapui5/ts-types": "^1.92.2",
    "eslint": "^7.32.0"
  },
  "engines": {
    "node": "16.16.0",
    "npm": "8.19.2"
  },
  "scripts": {
    "start": "cds run",
    "undeploy": "cf undeploy capinvoice --delete-services --delete-service-keys",
    "watch-createform": "cds watch --open createform/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "i18n": {
      "folders": [
        "_i18n",
        "i18n"
      ]
    },
    "requires": {
      "db": {
        "model": [
          "db",
          "srv"
        ],
        "kind": "hana",
        "pool": {
          "acquireTimeoutMillis": 600000,
          "min": 0,
          "max": 600000
        }
      },
      "uaa": {
        "kind": "xsuaa"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/invoice"
  ]
}
